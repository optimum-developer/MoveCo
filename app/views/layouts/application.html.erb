	<script type="text/javascript">
		$( window ).load(function() {
			setTimeout(function(){
				$('.dial_number').addClass('bounce');
			},2000);

		    setTimeout(function(){
			    $('.dial_number').removeClass('bounce');
			},3000);

			if(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
			    document.getElementById("dial_number_icon").style.display = "block";
			}
		});
		
		 // slider_div_default_width = (15/100*($(window).width())).toString() + "px"
   //      $('#hiddenPanel').css("width", slider_div_default_width);
         
		function call_me_back_details() {
			if($("#phone_number").val() !="" || $("#phone_number_2").val() !=""){
			// alert("wewhgyfg")
			if ($("#phone_number").val() !="")
			{
				form_data = $("#call_me_back").serialize();
			}
			else 
			{
				form_data = $("#call_me_back_2").serialize();
			}
		    	$.ajax({

		                type: "POST",
		                dataType: 'json',
		                url: '/home/call_me_back',
		                data: form_data,
		                success: function(data) {
		                	if(data["notice_status"]=="true"){
								$.notify("Thanks for your request. We will get back to you shortly.", "success");                                    
		                		$("#phone_number").val("")
		                		$("#phone_number_2").val("")
		                	}

		                	else{
		                		$.notify("Something Went Wrong! Please Try Again");
		                		$("#phone_number").val("")
		                		$("#phone_number_2").val("")
		                	}
		                	
		                },
		                error: function(err){
		                	$.notify("Something Went Wrong! Please Try Again");
		                }

		                })
		    }
		}
        $("#req_call_back_button").click(function() {
        $( "#toggle" ).toggle( "slide" );
        });

       
        var speed = 300;
        $('#close-bar').on('click', function(){
        	  if( $(window).width() > 948)
        	  {
        			slider_div_width = ($(window).width()/4).toString() + "px"
        			slider_closing_div_width = (($(window).width()/4) + 5).toString() + "px"
        			$('#hiddenPanel').css("padding-top", "1%");
        	  }	
        	  else if($(window).width() < 948 && $(window).width() > 475)
        	  {
        	  	slider_div_width = ($(window).width()/2).toString() + "px"
        			slider_closing_div_width = (($(window).width()/2) + 5).toString() + "px"
        			$('#hiddenPanel').css("padding-top", "2%");
        	  }
        	  else
        	  {
        	  	slider_div_width = (($(window).width()) - 20).toString() + "px"
        	  	slider_closing_div_width = ((($(window).width()) - 13) ).toString()+ "px"
        	  	$('#hiddenPanel').css("padding-top", "5%");
        	  }
            var $$ = $(this),
                panelWidth = $('#hiddenPanel').outerWidth();
                $(".request_number_field").show();
            if( $$.is('.myButton') ){
            		$('#hiddenPanel').css("width", slider_div_width);
                $('#hiddenPanel').animate({right:0}, speed);
                $$.removeClass('myButton')
            } else {
            		$('#hiddenPanel').css("width", slider_closing_div_width);
                $('#hiddenPanel').animate({right:-panelWidth}, speed);
                $$.addClass('myButton')
            }

        });

</script>

